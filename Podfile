# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'VhistaGPS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Firebase Pods for Travelia
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  
  # Google Places
  pod 'GooglePlaces'

  # Use AppShuttle Utilities
  pod 'AppShuttle'
  
  # Use ARKit Frameworks
  pod 'ARCL'
  
  #UI Enhance FrameWorks
  pod 'Sonar'
  
  #Network Utilities
  pod 'AFNetworking'

end
